<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.liquibase.org/xml/ns/dbchangelog">
    <changeSet id="create table request_data" author="U.Goryntsev">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="request_date"/>
            </not>
        </preConditions>
        <createTable tableName="request_data">
            <column name="id" type="SERIAL8"/>
            <column name="ip" type="text"/>
        </createTable>
    </changeSet>
</databaseChangeLog>